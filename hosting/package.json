    "history": "^4.10.1",
    "i18next": "^19.3.2",
    "i18next-browser-languagedetector": "^4.0.2",
    "lodash": "^4.17.15",
    "material-table": "^1.57.2",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "node-sass": "^4.13.1",
